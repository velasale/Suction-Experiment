<launch>

  <!-- Launch robot -->
  <include file="$(find apple_proxy)/launch/ur5e_bringup.launch">
    <arg name="robot_ip" value="169.254.174.50"/>
    <arg name="kinematics_config" default="$(find apple_proxy)/calibration/joes_ur5e_calibration.yaml"/>
  </include>
  

  <!-- Launch lab setup with moveIt -->
  <!-- Remember that line 27 of pickApp_real.launch is commented, to avoid the node issue-->
  <include file="$(find apple_proxy)/launch/pickApp_real.launch">
  </include>
  

  <!-- Launch Camera -->
  <include file="$(find video_stream_opencv)/launch/camera.launch">
    <arg name="video_stream_provider" value="/dev/video0"/>
  </include>


  <!-- Launch ROS Serial -->
  <node pkg="rosserial_python" type="serial_node.py" name="rosserial" output="screen">
    <param name="port" value="/dev/ttyACM1" />
    <param name="baud" value="57600" />
   </node>


</launch>